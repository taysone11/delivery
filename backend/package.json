{
  "name": "sushi-delivery-backend",
  "version": "0.1.0",
  "private": true,
  "description": "Backend service for sushi delivery app",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "db:seed": "tsx db/seeds/seed.ts",
    "db:seed:clear": "tsx db/seeds/clear.ts",
    "db:seed:fresh": "yarn db:seed:clear && yarn db:seed"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22.15.30",
    "@types/pg": "^8.15.5",
    "tsx": "^4.20.5",
    "typescript": "^5.8.3"
  }
}
